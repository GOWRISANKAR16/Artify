{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "allowJs": true,
      "checkJs": false,
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      },
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  